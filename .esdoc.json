{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test/",
          "interfaces": [
            "test",
            "t.test"
          ],
          "includes": [
            "\\.spec\\.js$"
          ]
        },
        "lint": {
          "enable": true
        },
        "coverage": {
          "enable": true
        },
        "accessor": {
          "access": [
            "public"
          ],
          "autoPrivate": true
        }
      }
    }
  ]
}